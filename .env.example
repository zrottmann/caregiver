# Appwrite Configuration
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_PROJECT_ID=your-project-id
APPWRITE_API_KEY=your-api-key

# Database Configuration
APPWRITE_DATABASE_ID=your-database-id
APPWRITE_APPOINTMENTS_COLLECTION_ID=appointments-collection-id
APPWRITE_PATIENTS_COLLECTION_ID=patients-collection-id
APPWRITE_CAREGIVERS_COLLECTION_ID=caregivers-collection-id
APPWRITE_MESSAGES_COLLECTION_ID=messages-collection-id

# Functions Configuration
